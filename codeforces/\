#include<bits/stdc++.h>
using namespace std;
#include <iostream>

int main()
{
    int n; cin >> n;
    vector<int> arr(n);
    for (auto &x : arr) {
        cin >> x;
    }
    int tmp = arr[0];
    cout << tmp << " ";
    for (int i = 1; i < n; i++) {
        tmp |= arr[i];
        cout << tmp << " ";
    }
    return 0;
}

